  "browserslist": {
    "production": [
      "last 5 chrome version",
      "last 5 firefox version",
      "last 5 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/main/react/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/main/react/*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{yml,json,md,html}": [
      "prettier --write"
    ]
